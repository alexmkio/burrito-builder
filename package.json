{
  "name": "burrito-builder",
  "homepage": "https://alexmkio.github.io/burrito-builder/",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -b gh-pages -d build",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:cov": "vitest run --coverage && open './coverage/index.html'",
    "test:ci": "vitest run"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.6.1"
  },
  "devDependencies": {
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^3.0.0",
    "@vitest/coverage-c8": "^0.26.0",
    "gh-pages": "^4.0.0",
    "history": "^5.3.0",
    "jsdom": "^20.0.3",
    "sass": "^1.57.0",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vitest": "^0.26.0"
  }
}
